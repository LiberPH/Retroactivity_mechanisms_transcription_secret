begin model
	
	begin parameters
	
	tau=10
	kon ((3*10^8)*60.0)/((6.022*(10.0^23)*(42.0*(10.0^(-15)))))
	koff 30*60
	s 50.0
	d 0.02
	d2 0.06
	d3 0.02 
	d5 d3
	k 0.09/50
	k2 0.8
	a 0.01
	k3 0.5
	kdim 1.6*(10.0^7)*60.0/((6.022*(10.0^23)*(42.0*(10.0^(-15)))))
	kundim 0.05*60

end parameters

begin molecule types
	#Isolated
	S(b) #Signaling component it has one DNA binding site l
	A(b) #Autoactivator it has also one DNA binding site to its own promoter
	AaP(b)  #Autoactivator promoter has 8 potential sites to be bound by TF. 
	#A single binding can change the whole promoter to an active transcription form
	Null()
	RNA()
	RP(b)
	AA(b)
end molecule types

begin seed species
	#Isolated
	S(b) s
	A(b) 0
	AaP(b) 1
	$Null() 1
	RNA() 0
	RP(b) 300
	AA(b) 0
end  seed species

begin observables
	Molecules A A(b)
	Molecules AA AA(b)
	Molecules C1 RP(b!1).AA(b!1)
	Molecules S S(b)
	Molecules RNA RNA()
	
end observables

begin reaction rules

	##Null -> As(c) alpha

	AaP(b) -> AaP(b) + RNA() a
	
	AaP(b) + S(b) -> AaP(b) + S(b) + RNA() k

	A(b) + A(b) <-> AA(b) 2*kdim,kundim
	
	RNA() -> RNA() + A(b) k2
	
	RP(b) + AA(b) <-> RP(b!1).AA(b!1) kon, koff
	
	RNA() -> Null  d2
	
	A(b) -> Null d
	
	AA(b) -> Null d3
	
	RP(b!1).AA(b!1) -> RP(b) d3
end reaction rules
end model

#ACTIONS

generate_network({overwrite=>1});
#simulate_ode({t_end=>1440,n_steps=>1440,verbose=>1});

simulate_ssa({t_end=>1440,n_steps=>2880});

